{
  "name": "wawiz",
  "description": "Bring your whatsapp to the next level",
  "version": "0.1.0",
  "private": true,
  "build": {
    "appId": "com.wawiz.wawiz",
    "productName": "Wawiz",
    "copyright": "Copyright Â© 2021",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^1.0.3",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "axios": "^0.21.0",
    "electron-is-dev": "^1.0.1",
    "electron-store": "^6.0.1",
    "express": "^4.17.1",
    "firebase": "^8.2.0",
    "framer-motion": "^2.9.5",
    "nedb": "^1.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "sharp": "^0.26.3",
    "venom-bot": "^2.1.2"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build && tsc ./src/api/*.ts --outDir ./public/api",
    "build-api": "tsc ./src/api/*.ts --outDir ./public/api",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-pack": "yarn build && rm -rf dist && build -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "concurrently": "^4.1.0",
    "electron": "^4.0.6",
    "electron-builder": "^20.38.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "wait-on": "^3.2.0"
  },
  "rescripts": [
    "env"
  ]
}
